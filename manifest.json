{
  "name": "derpy.me link shortener.",
  "description": "The even more derpier link shortener, now with Chrome!",
  "version": "0.2",
  "manifest_version": 2,
  "background": {"page": "background.html"},
  "permissions": [
    "tabs", "contextMenus",
    "http://api.derpy.me/"
  ],
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icon16.png",
    "default_title": "derpy.me"//,
    //"default_popup": "popup.html"
  }
}
